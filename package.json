{
  "name": "fluid-simulation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:full": "npm run build:lib && npm start -- --port=4404 --host=0.0.0.0",
    "build": "ng build",
    "build:lib": "npm run ng build fluid-simulation-lib --prod",
    "test": "npm run test:lib && npm run test:example",
    "test:lib": "ng test fluid-simulation-lib --no-watch --progress=false --code-coverage --browsers ChromeHeadless",
    "test:example": "ng test example-app --no-watch --progress=false --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint fluid-simulation-lib --fix",
    "e2e": "ng e2e example-app",
    "postversion": "git push --follow-tags"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "core-js": "^3.1.3",
    "rxjs": "~6.5.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@angular/language-service": "~12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/webgl2": "~0.0.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "rxjs-marbles": "^5.0.2",
    "ts-node": "~8.2.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
